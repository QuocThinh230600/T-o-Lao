{
  "manifest_version": 3,
  "name": "supper display none",
  "version": "1.2",
  "description": "CSS + JS - update hidden thông báo on zalo, ẩn thông báo nổi trên shopee khi sử dụng ex so sánh giá",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://shopee.vn/*"
      ],
      "css": [
        "styles.css"
      ]
    },
    {
      "matches": [
        "https://chat.zalo.me//*"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
